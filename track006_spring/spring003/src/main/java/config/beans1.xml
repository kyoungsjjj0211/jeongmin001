<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.3.xsd">
		<!-- IceCreamShop 빈등록  - name에는 베라31,   
		 iceCream  choco로 등록 -->
	<!-- White white = new White() -->
	<bean id="white" class="spring003_di.White" />
	<bean id="choco" class="spring003_di.Choco" />


	<bean id="iceCreamShop" class="spring003_di.IceCreamShop">

		<property name="name" value="베라31"></property>
		<property name="iceCream" ref="choco"></property>

	</bean>

</beans>

